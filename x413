local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()local b=a.CreateLib("KILL NOOBS FOR NO FUCKING REASON SIMULATOR","DarkTheme")local c=b:NewTab("Main")local d=c:NewSection("HACKS FOR KILL NOOB SIM OR WHATEVER")d:NewDropdown("AutoSwing For DarkHeart (have weapon out)","unequip to stop",{"AutoSwing"},function(e)while 1+1==2 do local f=game:GetService("Workspace").Swatt3rz.TEAM;local g=game:GetService("Workspace").Swatt3rz.DarkHeart["AttackScript_Event"]g:FireServer(f)wait(.1)end end)print("AutoSwing Is On")d:NewDropdown("AutoSwing For DoomBringer (have weapon out)","unequip to stop",{"AutoSwing"},function(e)while 1+1==2 do local f=game:GetService("Workspace").Swatt3rz.TEAM;local g=game:GetService("Workspace").Swatt3rz.Doombringer["AttackScript_Event"]g:FireServer(f)wait(.1)end end)print("AutoSwing Is On")d:NewDropdown("AutoSwing For Fist (have Fist out)","unequip to stop",{"AutoSwing"},function(e)while 1+1==2 do local f=game:GetService("Workspace").Swatt3rz.TEAM;local g=game:GetService("Workspace").Swatt3rz.Fist["AttackScript_Event"]g:FireServer(f)wait(.1)end end)print("AutoSwing Is On")d:NewDropdown("AutoSwing For Scythe (have weapon out)","unequip to stop",{"AutoSwing"},function(e)while 1+1==2 do local f=game:GetService("Workspace").Swatt3rz.TEAM;local g=game:GetService("Workspace").Swatt3rz.Scythe["AttackScript_Event"]g:FireServer(f)wait(.1)end end)print("AutoSwing Is On")d:NewSlider("Walkspeed","HMMMM IT CHANGES THE SPEED MORON",250,16,function(e)game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=e end)d:NewSlider("Jumppower","HMMMM IT CHANGES THE JUMPPOWER MORON",250,50,function(e)game.Players.LocalPlayer.Character.Humanoid.JumpPower=e end)local h=b:NewTab("Credits")local i=h:NewSection("Made By Veteran#7796")local i=h:NewSection("This is a small project nothing to crazy.")i:NewButton("Copy Owner Discord Tag","Puts Owners Discord Tag on clipboard",function(e)local j=Instance.new("Sound",game:GetServic